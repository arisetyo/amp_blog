{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"minify"
	],
	"plugins": [
		["@babel/plugin-proposal-decorators", {"legacy": true}],
		["@babel/plugin-proposal-object-rest-spread"],
		"transform-class-properties",
		"transform-react-stateless-component-name",
		"react-css-modules"
	],
	"env": {
		"production": {
			"presets": [
				["minify", {
					"evaluate": false,
					"removeUndefined": false
				}]
			],
			"comments": false
		},
		"test": {
			"presets": [
				"@babel/preset-env",
				"@babel/preset-react"
			],
			"plugins": [
				["@babel/plugin-proposal-decorators", {"legacy": true}],
				["@babel/plugin-proposal-object-rest-spread"],
				"transform-class-properties",
				"transform-react-stateless-component-name",
				"react-css-modules"
			]
		}
	}
}